{
    "manifest_version": 2,

    "name": "Jira Worklog Tool",
    "version": "0.1",
    "author": "alfeugds",
    "description":
        "This extension allows the user to log the work in Jira easily.",
    "homepage_url": "https://github.com/alfeugds/jiraworklogtool",
        "icons": {
          "128": "icon-time-task-512x512.png"
        },
    "options_page": "options.html",
    "browser_action": {
        "default_icon": "icon-time-task-512x512.png",
        "default_popup": "popup.html",
        "default_title": "Log Work"
    },
    "permissions": ["activeTab", "storage","cookies","<all_urls>"]
    
}
